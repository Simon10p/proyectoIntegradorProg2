<%- include('./partials/head') %>

	<!-- Header -->
<%- include('./partials/header') %>
	<!-- /Header -->

	<!-- Novedades -->
    <main>
	<div class="container products-wrapper">
		<div class="row">
			<div class="col-12">
				<h2 class="register-title">Editar producto</h2>
			</div>
			<!-- Show the "ultimos" products -->
			<form action="/users/profile/<%= producto.usuario_id %>" method="POST" id="registerForm">
				<input type="text" name="nombre_producto" value="<%= producto.nombre_producto %>">
				<input type="text" name="descripcion" value="<%= producto.descripcion %>">
				<input type="text" name="img_url" value="<%= producto.img_url %>">
				<button type="submit">Confrimar cambios</button>
			</form>

<!-- "aca tiene que ir la informacion que viene desde el register" -->

    </main>
<%- include('./partials/footer') %>
